{
    "name": "archilex/filament-filter-sets",
    "description": "Advanced Tables, previously Filter Sets, supercharges your Filament Tables with user customizable views, enhanced filter tabs, reorderable columns, convenient view management, and more.",
    "keywords": [
        "archilex",
        "laravel",
        "filament",
        "filters",
        "views",
        "tables",
        "advanced-tables"
    ],
    "homepage": "https://github.com/archilex/filament-filter-sets",
    "license": "proprietary",
    "authors": [
        {
            "name": "Kenneth Sese",
            "email": "kmsese@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/tables": "^5.1",
        "illuminate/contracts": "^11.15|^12.0",
        "spatie/eloquent-sortable": "^4.2",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "filament/filament": "^5.1",
        "larastan/larastan": "^3.2",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.1",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "spatie/laravel-ray": "^1.29"
    },
    "autoload": {
        "psr-4": {
            "Archilex\\AdvancedTables\\": "src",
            "Archilex\\AdvancedTables\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Archilex\\AdvancedTables\\Tests\\": "tests"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "test:pest": "vendor/bin/pest --parallel",
        "test:sqlite": "vendor/bin/pest --configuration=phpunit.sqlite.xml",
        "test:mysql": "vendor/bin/pest --configuration=phpunit.mysql.xml",
        "test:pgsql": "vendor/bin/pest --configuration=phpunit.pgsql.xml",
        "test": [
            "@test:sqlite",
            "@test:phpstan"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Archilex\\AdvancedTables\\AdvancedTablesServiceProvider"
            ],
            "aliases": {
                "AdvancedTables": "Archilex\\AdvancedTables\\Facades\\AdvancedTables"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
